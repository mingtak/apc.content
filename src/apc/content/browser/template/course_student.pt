<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="apc.content">
<body>
<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">
<div class="row" tal:repeat="item view/getStudents">
  <tal:define tal:define="obj item/getObject;
                          title obj/title;
                          student obj/studentList">
    <tal:condition tal:condition="student">
      <div class="col-xs-12 col-sm-4 ">
        <p>${title}</p>
      </div>

      <div class="col-xs-12 col-sm-6">
        <p style="white-space: pre-line;">${student}</p>
      </div>
      <div class="col-xs-12" style="border-bottom: 2px solid #E0DEDE;"></div>
    </tal:condition>
  </tal:define>
</div>
</metal:content-core>
</metal:content-core>

</body>
</html>


