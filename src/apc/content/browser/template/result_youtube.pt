    <tr>
      <th>關鍵字</th>
      <th>族語</th>
      <th>影片</th>
    </tr>
    <tr tal:repeat="item result">
      <td>${python: item['keyword']}</td>
      <td>${python: item['language']}</td>
      <td>${python: item['embedUrl']}</td>
    </tr>
  </tal:def>
</table>

